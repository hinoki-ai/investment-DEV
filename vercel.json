{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "prism/web/dist",
  "framework": "vite",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/releases/(.*)",
      "destination": "/releases/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}